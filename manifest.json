{
    "manifest_version": 2,
    "name": "PW Helper by AiR",
    "version": "1.2",
    "description": "Physics Wallah Helper Extension",
    "icons": {
      "48": "icon/pw.png",
      "128": "icon/pw.png"
    },
    "permissions": ["storage", "https://www.pw.live/*"],
    "browser_action": {
      "default_icon": {
        "48": "icon/pw.png",
        "128": "icon/pw.png"
      },
      "default_title": "PW Helper",
      "default_popup": "merged.html"
    }
  }
  